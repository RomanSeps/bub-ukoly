#include <stdio.h>

int main() {
    int a1 = 3; //aritmetick치
    int d = 3;
    printf("aritmetick치 ");
    for(int n = 1; n <= 10; n++){
        a1 = a1 + d;
        printf("%i, ", a1);
    }

    int b1 = 3; // geometrick치
    int q = 2; 
    printf("\n geometrick치 ");
    for(int m = 1; m <= 10; m++){
        b1 = b1 * q;
        printf("%i, ", b1);
    }
    return 0;
}
